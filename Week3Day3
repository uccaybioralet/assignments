		System.out.println("I will learn \"" + lan1 + "\" and \"" + lan2 + "\" at Cybertek School.");
		

	}

}

Question 9

package day_3_assignments;

public class question3 {

	public static void main(String[] args) {
		byte steps = 100;
		short miles = 5000;
		int count = 1_000_000;
		long population = 3434455667l;
		
		
		System.out.println(steps + " " + miles + " " + count + " " + population);
		

	}

}

Question 10

package day_3_assignments;

public class question3 {

	public static void main(String[] args) {
		
		byte hour = 10;
		byte minute = 15;
		byte second = 40;
		String amorPm = "pm";
		
		
		System.out.println(hour + ":" + minute + ":" + second + " " + amorPm);
		

	}

}
