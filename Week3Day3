Question 6

package day_3_assignments;

public class Question4 {

	public static void main(String[] args) {
		String seatL, seat2, seat3, seat4, seatR, cs;
		
		seatL="Bob";
		seat2="James";
		seat3="Betty";
		seat4="Herb";
		seatR="Jill";
		cs = ", ";
		
		System.out.println(seatL + cs + seat2 + cs + seat3 + cs + seat4 + cs + seatR);

	}

}

